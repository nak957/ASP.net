@model List<PTQ_lesson05.Models.PtqCustomer>
    @{
        ViewBag.Title = "Danh sách khách hàng";
        //var list = ViewBag.list;
        //var list = Model as List<PTQ_lesson05.Models.PtqCustomer>;
        int stt = 0;
    }

    <h2>Danh sách khách hàng</h2>
    <div class="row">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>#</th>
                    <th>Mã số</th>
                    <th>Họ tên</th>
                    <th>Địa chỉ</th>
                    <th>Năm sinh</th>
                    <th>chức năng</th>
                </tr>
            </thead>
            <tbody>
                @*@foreach (var item in list)*@
                @foreach (var item in Model)
                {
                    stt++;
                    <tr>
                        <td>@stt</td>
                        <td>@item.customerId</td>
                        <td>@item.firstName @item.LastName</td>
                        <td>@item.Address</td>
                        <td>@item.YearOfBirth</td>
                        <td>
                            <a href="#">Edit</a>
                            <a href="#">Delete</a>
                        </td>
                    </tr>
                }

            </tbody>
            </table">
    </div>
